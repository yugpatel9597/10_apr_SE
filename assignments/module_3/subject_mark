#include<stdio.h>
int main()
{
    int maths,science,ss,english,gujrati;
    int totalmarks;
    printf("\nenter the maths marks:");
    scanf("%d",&maths);
    printf("\nenter the science marks:");
    scanf("%d",&science);
    printf("\nenter the ss marks:");
    scanf("%d",&ss);
    printf("\nenter the english marks:");
    scanf("%d",&english);
    printf("\nenter the gujrati marks:");
    scanf("%d",&gujrati);
    totalmarks = maths + science + ss + english + gujrati;
    printf("\ntotal marks is:%d",totalmarks);
    float percentage = (totalmarks / 5) ;
    printf("\npercentage is:%f",percentage);
    if (percentage > 75)
    {
        printf("\nditinction");
    }
    else if(percentage > 60 && percentage <= 75)
    {
        printf("\nfirst class");
    }
    else if(percentage > 50 && percentage <= 60)
    {
        printf("\nsecond class");
    }
    else if(percentage > 35 && percentage <= 50)
    {
        printf("\npass class");
    }
    else
    {
        printf("\nfail");
    }


}
