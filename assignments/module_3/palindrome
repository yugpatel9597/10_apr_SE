#include<stdio.h>
int main()
{
    int n,a,b,c,d;
    printf("enter a four digit no:");
    scanf("%d",&n);
    a = n/1000;
    b = (n/100) - (a*10);
    c =((n/10) - (a*100) - (b*10));
    d = (n - (a*1000) - (b*100) - (c*10));
    if(a==d&&b==c)
    {
        printf("%d : is pallindrome",n);
    }
    else
    {
        printf("%d : is not pallindrome",n);
    }

}
